<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="poicategory" pattern="/">
        <default key="_controller">GmapsBundle:PoiCategory:index</default>
    </route>

    <route id="poicategory_show" pattern="/{id}/show">
        <default key="_controller">GmapsBundle:PoiCategory:show</default>
    </route>

    <route id="poicategory_new" pattern="/new">
        <default key="_controller">GmapsBundle:PoiCategory:new</default>
    </route>

    <route id="poicategory_create" pattern="/create">
        <default key="_controller">GmapsBundle:PoiCategory:create</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="poicategory_edit" pattern="/{id}/edit">
        <default key="_controller">GmapsBundle:PoiCategory:edit</default>
    </route>

    <route id="poicategory_update" pattern="/{id}/update">
        <default key="_controller">GmapsBundle:PoiCategory:update</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="poicategory_delete" pattern="/{id}/delete">
        <default key="_controller">GmapsBundle:PoiCategory:delete</default>
        <requirement key="_method">post</requirement>
    </route>

</routes>
